{
	"name": "Dashiki",
	"version": "0.1.0",
	"author": [
		"Dan Andreescu"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Dashiki",
	"descriptionmsg": "dashiki-desc",
	"type": "other",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.31.0",
		"extensions": {
			"JsonConfig": "*"
		}
	},
	"MessagesDirs": {
		"Dashiki": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"Dashiki\\": "includes/"
	},
	"JsonConfigModels": {
		"JsonConfig.Dashiki": {
			"view": "Dashiki\\DashikiView",
			"class": "JsonConfig\\JCContent"
		}
	},
	"JsonConfigs": {
		"JsonConfig.Dashiki": {
			"pattern": "/^Dashiki\\:./",
			"isLocal": true
		}
	},
	"manifest_version": 1
}
